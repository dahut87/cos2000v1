all:
	cd lib
	make
	cd ..

	cd noyau
	make
	cd ..

	cd install
	make
	cd ..

	cd commande
	make
	cd ..

	cd programs
	make
	cd ..

copy: clean all
	copy noyau\systeme.sys b:
      copy commande\*.ce b:
      copy lib\*.lib b:
      copy programs\*.ce b:

clean:
	cd lib
	make clean
	cd ..

	cd noyau
	make clean
	cd ..

	cd install
	make clean
	cd ..

	cd commande
	make clean
	cd ..

	cd programs
	make clean
	cd ..